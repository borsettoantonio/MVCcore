{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "Data Source=Data/MyCourse.db"
  },
  "Courses": {
    "PerPage": 10,
    "Order": {
      "By": "Rating",
      "Ascending": true,
      "Allow": [
        "Title",
        "Rating",
        "CurrentPrice"
      ]
    },
    "CacheDuration" : 60
  },
  "MemoryCache": {
    "SizeLimit": 1000
  },
  "ResponseCache":{
    "Home":{
      "Duration":60,
      "Location":"Any",
      "VaryByQueryKeys":["search","page","orderby","ascending"]
    }
  }
}